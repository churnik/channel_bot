[loggers]
keys=root,channel,database,telethon

[handlers]
keys=consoleHandler,fileHandler

[formatters]
keys=simpleFormatter

[logger_root]
level=DEBUG
handlers=consoleHandler,fileHandler
propagate=1

[logger_channel]
level=INFO
handlers=consoleHandler,fileHandler
propagate=0
qualname=channel

[logger_database]
level=INFO
handlers=consoleHandler,fileHandler
propagate=0
qualname=database

[logger_telethon]
level=WARNING
handlers=consoleHandler
propagate=1
qualname=telethon

[handler_consoleHandler]
class=StreamHandler
level=WARNING
formatter=simpleFormatter
args=(sys.stdout,)

[handler_fileHandler]
class=handlers.RotatingFileHandler
level=INFO
formatter=simpleFormatter
args=("bot.log","a",10240,4)

[formatter_simpleFormatter]
format=%(asctime)s - %(name)s - %(levelname)s - %(message)s
datefmt=